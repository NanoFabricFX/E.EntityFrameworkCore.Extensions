    <RootNamespace>E</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Version>0.1.0</Version>
    <Authors>staneee</Authors>
    <Product />
    <NeutralLanguage>zh-CN</NeutralLanguage>
    <Copyright>staneee</Copyright>
    <RepositoryUrl>https://github.com/staneee/E.EntityFrameworkCore.Extensions</RepositoryUrl>
    <RepositoryType>github</RepositoryType>
    <PackageProjectUrl>https://github.com/staneee/E.EntityFrameworkCore.Extensions</PackageProjectUrl>
    <PackageTags>E,Data,EntityFrameworkCore,EFCore,Core</PackageTags>
